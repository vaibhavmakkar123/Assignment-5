list1=[]
for i in range (10):
    t=int(input("enter integer number" + str(i+1) + ":"))
    list1.append(t)
print(list1)

print("the positive numbers in the list are:")
for t in list1 :
    if t>0 :
        print(t)

print("the negative numbers in the list are ")
for t in list1 :
    if t<0 :
        print(t)


print("the odd numbers in the list are ")
for t in list1:
    if t%2 != 0:
        print(t)

print("the even numbers in the list are:")
for t in list1:
    if t%2 ==0 :
        print(t)

number_set=[*set(list1)]
for i in number_set :
    print(i,"occurs",list1.count(i),"times")
